{
  "name": "visa-mcp",
  "version": "0.0.1",
  "author": "",
  "license": "ISC",
  "description": " VISA Model Configuration Protocol (MCP) client for Node.js",
  "keywords": [
    "visa",
    "mcp",
    "client",
    "nodejs"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/src/app.js"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer"
      ],
      [
        "@semantic-release/release-notes-generator"
      ],
      [
        "@semantic-release/changelog"
      ],
      [
        "@semantic-release/npm"
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ðŸš€ ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.17",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.8.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "winston": "^3.17.0"
  }
}
